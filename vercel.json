{
    "routes": [
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1",
        "headers": {
          "Cache-Control": "s-maxage=3600"
        }
      }
    ]
  }